<view data-event-opts="{{[['tap',[['clickCoupon',['$event']]]]]}}" class="coupon-item clearfix" bindtap="__e"><image class="coupon-pic" src="{{coupon.thumbnail_pic}}" lazy-load="true"></image><view class="coupon-info"><view class="coupon-title"><block wx:if="{{coupon.platform_id==3&&coupon.jd_activity_type==1}}"><image class="jdpg-icon" src="/static/image/common/jdpg.png"></image></block><block wx:if="{{coupon.platform_id==3&&coupon.jd_activity_type==2}}"><image class="jdms-icon" src="/static/image/common/jdms.png"></image></block><block wx:if="{{coupon.platform_id==3&&coupon.jd_activity_type==0}}"><image class="jd-icon" src="/static/image/common/jd.png"></image></block><block wx:if="{{coupon.platform_id==4}}"><image class="pdd-icon" src="/static/image/common/pdd.png"></image></block><block wx:if="{{coupon.platform_id==6}}"><image class="kl-icon icon" src="/static/image/common/kl.png"></image></block><block wx:if="{{coupon.platform_id==7}}"><image class="wph-icon icon" src="/static/image/common/wph.png"></image></block>{{coupon.title+''}}</view><view class="coupon-fee-group"><block wx:if="{{coupon.ticket&&coupon.ticket.coupon_price>0}}"><view class="fee-quan"><view class="quan">券</view><view class="quan-money">{{coupon.ticket.coupon_price+"元"}}</view></view></block></view><view class="coupon-price-area"><view class="final-price"><text class="rmb">￥</text><text class="price">{{coupon.zk_price}}</text></view><block wx:if="{{coupon.zk_price!=coupon.raw_price}}"><view class="origin-price"><text class="price">{{coupon.raw_price}}</text></view></block></view><block wx:if="{{coupon.month_sales}}"><block><block wx:if="{{coupon.month_sales=='新品上新'}}"><view class="sale-num">{{''+coupon.month_sales+''}}</view></block><block wx:else><view class="sale-num">{{''+coupon.month_sales+'人付款'}}</view></block></block></block></view></view>